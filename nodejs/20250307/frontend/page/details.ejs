


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://unpkg.com/axios@1.6.7/dist/axios.min.js"></script>
</head>
</head>
<body>
    <form id="update">

        <div><%= token %></div>
        <input type="text" name="uuid">
        <div></div>
        <button>수정</button>
    </form>
</body>
<script>
    const Updatedata = async (e) => {
        e.preventDefault();
        const {uuid : {value : Uuidvalue}} = e.target;
        await axios.put('/details', {uuid : Uuidvalue}, 
        {withCredentials : true});

        location.href = '/details';
    }
    update.onsubmit = Updatedata;
</script>
</html>